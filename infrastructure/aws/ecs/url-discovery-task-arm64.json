{
  "family": "url-discovery-task",
  "networkMode": "awsvpc",
  "executionRoleArn": "arn:aws:iam::907929922976:role/ecsTaskExecutionRole",
  "taskRoleArn": "arn:aws:iam::907929922976:role/ecsTaskRole",
  "containerDefinitions": [
    {
      "name": "url-discovery-container",
      "image": "907929922976.dkr.ecr.eu-west-1.amazonaws.com/url-discovery-service:arm64-latest",
      "essential": true,
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "/ecs/url-discovery-task",
          "awslogs-region": "eu-west-1",
          "awslogs-stream-prefix": "ecs"
        }
      },
      "environment": [
        {
          "name": "ENVIRONMENT",
          "value": "aws"
        },
        {
          "name": "MAX_PAGES",
          "value": "20"
        },
        {
          "name": "AWS_REGION",
          "value": "eu-west-1"
        },
        {
          "name": "S3_BUCKET",
          "value": "website-analyzer-data"
        }
      ]
    }
  ],
  "requiresCompatibilities": [
    "FARGATE"
  ],
  "cpu": "512",
  "memory": "1024",
  "runtimePlatform": {
    "cpuArchitecture": "ARM64",
    "operatingSystemFamily": "LINUX"
  }
}
