{
    "name": "vuxi-monorepo",
    "version": "1.0.0",
    "private": true,
    "workspaces": [
        "packages/api",
        "packages/db",
        "packages/scrape+capture",
        "packages/ui"
    ],
    "scripts": {
        "dev:ui": "npm run dev -w packages/ui",
        "dev:api": "npm run dev -w packages/api",
        "db:migrate": "npm run db:migrate -w packages/db"
    },
    "devDependencies": {
        "dotenv-cli": "^8.0.0"
    }
}
